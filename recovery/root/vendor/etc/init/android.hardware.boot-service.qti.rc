service vendor.boot-qti /vendor/bin/hw/android.hardware.boot-service.qti
    class early_hal
    user root
    group root
    disabled
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/vendor/lib:/system/lib64:/system/lib:/sbin
    seclabel u:r:recovery:s0
